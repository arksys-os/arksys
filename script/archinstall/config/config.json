{
    "additional-repositories": [
        "multilib"
    ],
    "archinstall-language": "English",
    "audio": "pipewire",
    "bootloader": "grub-install",
    "desktop-environment": "kde",
    "debug": false,
    "desktop-enivronment": "kde",
    "gfx_driver": "Nvidia (open-source nouveau driver)",
    "harddrives": [
        "/dev/sda"
    ],
    "hostname": "arksys",
    "kernels": [
        "linux"
    ],
    "keyboard-layout": "es",
    "mirror-region": {},
    "nic": {
        "dhcp": true,
        "dns": null,
        "iface": null,
        "ip": null,
        "type": "nm",
        "NetworkManager": true,
        "nic": "Use NetworkManager (necessary to configure internet graphically in GNOME and KDE)"
    },
    "no_pkg_lookups": false,
    "ntp": true,
    "offline": false,
    "packages": [
        "docker",
        "git",
        "firefox",
        "obsidian",
        "telegram-desktop",
        "tree"
    ],
    "parallel downlaods": 0,
    "profile": {
        "path": "/usr/lib/python3.10/site-packages/archinstall/profiles/desktop.py"
    },
    "script": "guided",
    "silent": false,
    "swap": true,
    "sys-encoding": "utf-8",
    "sys-language": "en_US",
    "timezone": "Atlantic/Canary",
    "version": "2.5.1"
}
